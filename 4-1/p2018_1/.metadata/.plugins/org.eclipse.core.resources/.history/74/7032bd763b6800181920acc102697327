import java.io.BufferedWriter;
import java.io.FileWriter;
import java.util.ArrayList;

public class CSV {
	public int createCSV(String filepath, String filename ,String filename, String filepath) {
		DB db = new DB();
		
		int resultCount =0; 
		 
		ArrayList<String[]> data =  db.TestKeywordSelectData();
				

		try{
		    BufferedWriter fw = 
		            new BufferedWriter(new FileWriter(filepath+"/" + filename + ".csv", true));
		 
		     
		 
		for(String dom : list){         
		 
		     fw.write(dom+","+"test");
		 
		      fw.newLine();
		 
		      resultCount++;
		 
		    if(resultCount % 100 == 0) 
		 
		            log.info("resultCount :"+resultCount + "/" + list.size());
		 
		     }
		 
		             
		 
		    fw.flush();
		     // 객체 닫기
		    fw.close();
		 
		             
		 
		}catch (Exception e) {
		 
		  // TODO: handle exception
		 
		  e.printStackTrace();
		 
		}
		    return resultCount;
		}
	}
	
}
